
public class LogItem implements Comparable<LogItem>{
	int hours;
	int mins;
	int secs;
	String name;
	double size;
	String unit;
	
	public LogItem(int hours, int mins, int secs, String name, double size, String unit) {
		this.hours = hours;
		this.mins = mins;
		this.secs = secs;
		this.name = name;
		this.size = size;
		this.unit = unit;
	}
	
	public String toString(){
		return "\n"+hours+":"+mins+":"+secs+"  "+ name + "  " +size +  "  " +unit ; 
	}

	@Override
	public int compareTo(LogItem o) {
		if(o.hours-this.hours>0){
			return o.hours-this.hours;
		}
		else{
		return 0;
		}
	}


	
	
	
	
	
}
