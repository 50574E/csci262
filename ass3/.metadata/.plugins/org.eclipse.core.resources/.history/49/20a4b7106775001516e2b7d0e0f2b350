
public class LogItem {
	int hours;
	int mins;
	int secs;
	int name;
	int size;
	int unit;
	
	public LogItem(int hours, int mins, int secs, int name, int size, int unit) {
		this.hours = hours;
		this.mins = mins;
		this.secs = secs;
		this.name = name;
		this.size = size;
		this.unit = unit;
	}
	
	
	
	
}
